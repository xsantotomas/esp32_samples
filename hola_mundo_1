#include <Arduino.h>

//Digital GPIO - Digital Write 
//ejemplo de programa para encender y apagar led azul en el chip cada tres segundos 

void setup() {
  // put your setup code here, to run once:
 pinMode(2, OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(2, HIGH);
  delay(3000);
  digitalWrite(2, LOW);
  delay(3000);
}
